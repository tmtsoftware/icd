            "schema": {
              "type": "string"
            },
            "example": "A32"
          }
        ],
        "requestBody": {
          "description": "A range of dates, from, to, where each date has the format yyyy-mm-dd",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DateRange"
              },
              "example": {
                "from": "2020-10-23",
