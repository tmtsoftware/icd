              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "A list of objects mapping segment id to position on a given date",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/SegmentToM1Pos"
                  }
                },
                "example": [
                  {
                    "date": "2021-05-27",
                    "maybeId": "SN-484",
                    "position": "B78"
                  },
                  {
                    "date": "2021-05-27",
                    "position": "D78"
                  }
                ]
              }
            }
          },
          "400": {
            "description": "",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                },
                "example": "Error message."
              }
            }
          }
        }
      }
    },
